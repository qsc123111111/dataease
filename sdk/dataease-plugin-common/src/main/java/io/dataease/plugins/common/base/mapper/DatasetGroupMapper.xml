    <select id="page" resultMap="BaseResultMap">
      SELECT
      dg.id,
      dg.`name`,
      dg.data_name,
      dg.`desc`,
      dg.create_by,
      dg.create_time ,
      dt.`name` as object_name,
      dr.datamodel_id,
      (SELECT
      GROUP_CONCAT( dg.`name` SEPARATOR ', ' )
      FROM
      `datalabel_group` dg
      LEFT JOIN datalabel ON datalabel.group_id = dg.id
      LEFT JOIN datalabel_ref ON datalabel_ref.datalabel_id = datalabel.id
      where datamodel_id=dr.datamodel_id
      GROUP BY dr.datamodel_id) as label_ref
      FROM
      dataset_group dg
      LEFT JOIN datamodel dm on dg.id=dm.dataset_group_id
      LEFT JOIN dataset_table dt on dm.dataobject_id=dt.id
      LEFT JOIN datalabel_ref dr on dg.id=dr.datamodel_id
      <where>
        <if test="time != null">
          and dg.create_time >= #{time} and dg.create_time &lt; #{plusOneTime}
        </if>
        <if test="keyWord != null and keyWord != ''">
          and dg.`name` like concat('%',#{keyWord},'%')
        </if>
        and pid=#{id} and dir_type=1
      </where>
      <if test="order != null">
        order by concat('dataset_group.',#{order})
      </if>
      limit #{pageNo},#{pageSize}
    </select>
